", // Solo para l칤deres\r\n  });\r\n\r\n  const [message, setMessage] = useState(\"\"); // Mensaje de 칠xito o error\r\n  const [errors, setErrors] = useState({}); // Errores de validaci칩n\r\n  const [showPassword, setShowPassword] = useState(false); // Estado para mostrar/ocultar contrase침as\r\n\r\n  const navigate = useNavigate(); // Inicializar useNavigate para redirecci칩n\r\n\r\n  const handleChange = (e) => {\r\n    const { name, value } = e.target;\r\n    setFormData({ ...formData, [name]: value });\r\n  };\r\n\r\n  const validate = () => {\r\n    const newErrors = {};\r\n\r\n    if (!formData.nombre_completo.trim()) {\r\n      newErrors.nombre_completo = \"El nombre completo es obligatorio.\";\r\n    }\r\n\r\n    if (!formData.celular.trim()) {\r\n      newErrors.celular = \"El celular es obligatorio.\";\r\n    } else if (!/^\\d{10}$/.test(formData.celular)) {\r\n      newErrors.celular = \"El celular debe tener 10 d칤gitos.\";\r\n    }\r\n\r\n    if (!formData.correo.trim()) {\r\n      newErrors.correo = \"El correo es obligatorio.\";\r\n    } else if (!/\\S+@\\S+\\.\\S+/.test(formData.correo)) {\r\n      newErrors.correo = \"El formato del correo no es v치lido.\";\r\n    }\r\n\r\n    if (!formData.documento_identidad.trim()) {\r\n      newErrors.documento_identidad = \"El documento de identidad es obligatorio.\";\r\n    }\r\n\r\n    if (!formData.contrase침a.trim()) {\r\n      newErrors.contrase침a = \"La contrase침a es obligatoria.\";\r\n    } else if (formData.contrase침a.length < 8) {\r\n      newErrors.contrase침a = \"La contrase침a debe tener al menos 8 caracteres.\";\r\n    }\r\n\r\n    if (formData.contrase침a !== formData.confirmar_contrase침a) {\r\n      newErrors.confirmar_contrase침a = \"Las contrase침as no coinciden.\";\r\n    }\r\n\r\n    if (!formData.congregacion_actual.trim()) {\r\n      newErrors.congregacion_actual = \"La congregaci칩n actual es obligatoria.\";\r\n    }\r\n\r\n    if (!formData.distrito.trim()) {\r\n      newErrors.distrito = \"El distr